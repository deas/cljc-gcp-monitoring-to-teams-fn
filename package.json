{
  "name": "cljc-gcp-monitoring-to-teams-fn",
  "main": "./src/main/index-nbb.mjs",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "shadow-cljs": "2.15.12"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.9.0",
    "nbb": "0.1.1",
    "strftime": "^0.10.0"
  },
  "scripts": {
    "serve": "AUTH_TOKEN=s3cr3t functions-framework --signature-type=http --source ./dist/function --target=handle",
    "serve-nbb": "AUTH_TOKEN=s3cr3t functions-framework --signature-type=http --source ./src --target=handle",
    "start": "node dist/node/index.js",
    "pretest": "shadow-cljs compile :test",
    "test": "node dist/test.js",
    "watch": "shadow-cljs watch :node",
    "repl": "shadow-cljs cljs-repl :node",
    "check": "shadow-cljs check :cloud-function",
    "build": "shadow-cljs release :cloud-function --debug",
    "gcp-build": "npm run build",
    "predeploy": "npm run build",
    "deploy": "terraform apply"
  }
}
